    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/chain_analisys/main.py",
            "cwd": "${workspaceFolder}/chain_analisys",
            "console": "integratedTerminal",
            "args": [
                "--config",
                "../config",
                "--log-level",
                "INFO",
            ],
            "env": {"PYTHONPATH": "${cwd}"}
        }
    ]
}